<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper     
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">  
<!--singleTable Operate in FriceDB -->
<mapper namespace="FriceDBSingle">
	<resultMap id="gene2GoMap" type="Gene2Go">
		<result property="geneID" column="GeneID"/>  
		<result property="function" column="GoFunction"/>  
	</resultMap>
	<select id="selectGene2Go" parameterType="Gene2Go" resultMap="gene2GoMap">
		select *
		from gene2go
		where GeneID = #{geneID} 
		and GOID = #{GOID} 
	</select>
	<select id="selectGene2GoID" parameterType="long" resultMap="gene2GoMap">
		select *
		from gene2go
		where GeneID = #{id}
	</select>
	<insert id="insertGene2Go" parameterType="Gene2Go">
		insert into gene2go (GeneID,GOID,Evidence,Qualifier,GOTerm,Reference,GoFunction,DataBaseInfo)
		values (#{geneID},#{GOID},#{evidence},#{qualifier},#{GOTerm},#{reference},#{function},#{dataBase})
	</insert>
	<update id="updateGene2Go" parameterType="Gene2Go">
		update gene2go set
		Evidence = #{evidence},
		Reference = #{reference}
		where GeneID = #{geneID}
		and GOID = #{GOID} 
	</update>
	
	<resultMap id="NCBIIDMap" type="NCBIID">
		<result property="geneID" column="GeneID"/>
		<result property="dbInfo" column="DataBaseInfo"/>
	</resultMap>
	<select id="selectNCBIID" parameterType="NCBIID" resultMap="NCBIIDMap">
		select *
		from NCBIID
		<where>
			<if test="geneID !=0 and geneID !=null">
				GeneID = #{geneID} 
			</if>
			<if test="accessID !=null">
				and accessID = #{accessID} 
			</if>
			<if test="taxID !=0 and taxID !=null">
				and TaxID = #{taxID} 
			</if>
			<if test="dbInfo !=null">
				and DataBaseInfo = #{dbInfo}
			</if>
	    </where> 
	</select>
	<insert id="insertNCBIID" parameterType="NCBIID">
		insert into NCBIID (TaxID,GeneID,accessID,DataBaseInfo)
		values (#{taxID},#{geneID},#{accessID},#{dbInfo})
	</insert>
	<update id="updateNCBIID" parameterType="NCBIID">
		update NCBIID
		<set>
			<if test="taxID !=null and taxID !=0">
				TaxID = #{taxID},
			</if>
			<if test="geneID !=null and geneID !=0">
				GeneID = #{geneID},
			</if>
			<if test="dbInfo !=null">
				DataBaseInfo = #{dbInfo},
			</if>
			<if test="accessID !=null">
				accessID = #{accessID} 
			</if>
		</set>
		<where>
			<if test="geneID !=0 and geneID !=null">
				GeneID = #{geneID} 
			</if>
			<if test="accessID !=null">
				and accessID = #{accessID} 
			</if>
	    </where> 
	</update>
	
	<resultMap id="UniGene2GoMap" type="UniGene2Go">
		<result property="uniProtID" column="UniProtID"/> 
		<result property="function" column="GoFunction"/> 
	</resultMap>
	<select id="selectUniGene2Go" parameterType="UniGene2Go" resultMap="UniGene2GoMap">
		select *
		from UniGene2Go
		where UniProtID = #{uniProtID} 
		and GOID = #{GOID} 
	</select>
	<select id="selectUniGene2GoID" parameterType="string" resultMap="UniGene2GoMap">
		select *
		from UniGene2Go
		where UniProtID = #{uniProtID} 
	</select>
	<insert id="insertUniGene2Go" parameterType="UniGene2Go">
		insert into UniGene2Go (UniProtID,GOID,Evidence,Qualifier,GOTerm,Reference,GoFunction,DataBaseInfo)
		values (#{uniProtID},#{GOID},#{evidence},#{qualifier},#{GOTerm},#{reference},#{function},#{dataBase})
	</insert>
	<update id="updateUniGene2Go" parameterType="UniGene2Go">
		update UniGene2Go set
		Evidence = #{evidence},
		Reference = #{reference}
		where UniProtID = #{uniProtID}
		and GOID = #{GOID} 
	</update>
	
	<resultMap id="geneInfoMap" type="GeneInfo">
		<id property="geneID" column="GeneID" />
		<result property="symbol" column="Symbol"/>
		<result property="synonyms" column="Synonyms"/>
		<result property="symNome" column="SymbolFromNomenclature"/>
		<result property="fullNameNome" column="FullNameFromNomenclature"/>
		<result property="otherDesign" column="OtherDesignations"/>
		<result property="description" column="description"/>
		<result property="typeOfGene" column="typeOfGene"/>
		<result property="modDate" column="ModificationDate"/>
		<result property="nomStat" column="NomenclatureStatus"/>
		<result property="modDate" column="ModificationDate"/>
	</resultMap>
	<select id="selectGeneInfo" parameterType="GeneInfo" resultMap="geneInfoMap">
		select *
		from geneInfo
		where GeneID = #{geneID} 
	</select>
	<select id="selectGeneInfoID" parameterType="long" resultMap="geneInfoMap">
		select *
		from geneInfo
		where GeneID = #{id}
	</select>
	<insert id="insertGeneInfo" parameterType="GeneInfo">
		insert into geneInfo (GeneID,Symbol,LocusTag,Synonyms,dbXrefs,chromosome,mapLocation,description,typeOfGene,SymbolFromNomenclature,FullNameFromNomenclature,NomenclatureStatus,OtherDesignations,ModificationDate)
		values (#{geneID},#{symbol},#{locusTag},#{synonyms},#{dbXrefs},#{chromosome},#{mapLocation},#{description},#{typeOfGene},#{symNome},#{fullNameNome},#{nomStat},#{otherDesign},#{modDate})
	</insert>
	<update id="updateGeneInfo" parameterType="GeneInfo">
		update geneInfo
		<set>
			<if test="geneID !=null and geneID != 0">
				GeneID = #{geneID},
			</if>
			<if test="symbol !=null and symbol != ''">
				Symbol = #{symbol},
			</if>
			<if test="locusTag !=null and locusTag != '' ">
				LocusTag = #{locusTag},
			</if>
			<if test="synonyms !=null and locusTag != '' ">
				Synonyms = #{synonyms},
			</if>
			<if test="dbXrefs !=null and locusTag != '' ">
				dbXrefs = #{dbXrefs},
			</if>
			<if test="chromosome !=null and chromosome != '' ">
				chromosome = #{chromosome},
			</if>
			<if test="mapLocation !=null and mapLocation != '' ">
				mapLocation = #{mapLocation},
			</if>
			<if test="description !=null and description != '' ">
				description = #{description},
			</if>
			<if test="typeOfGene !=null and typeOfGene != '' ">
				typeOfGene = #{typeOfGene},
			</if>
			<if test="symNome !=null and symNome != '' ">
				SymbolFromNomenclature = #{symNome},
			</if>
			<if test="fullNameNome !=null and fullNameNome != '' ">
				FullNameFromNomenclature = #{fullNameNome},
			</if>
			<if test="nomStat !=null and nomStat != '' ">
				NomenclatureStatus = #{nomStat},
			</if>
			<if test="otherDesign !=null and otherDesign != '' ">
				OtherDesignations = #{otherDesign},
			</if>
			<if test="modDate !=null and modDate != '' ">
				ModificationDate = #{modDate}
			</if>
		</set>
		where GeneID = #{geneID} 
	</update>
	
	<resultMap id="UniGeneInfoMap" type="UniGeneInfo">
		<id property="uniProtID" column="UniProtID" />
		<result property="symbol" column="Symbol"/>
		<result property="synonyms" column="Synonyms"/>
		<result property="symNome" column="SymbolFromNomenclature"/>
		<result property="fullNameNome" column="FullNameFromNomenclature"/>
		<result property="otherDesign" column="OtherDesignations"/>
		<result property="description" column="description"/>
		<result property="typeOfGene" column="typeOfGene"/>
		<result property="modDate" column="ModificationDate"/>
		<result property="nomStat" column="NomenclatureStatus"/>
		<result property="modDate" column="ModificationDate"/>
	</resultMap>
	<select id="selectUniGeneInfo" parameterType="UniGeneInfo" resultMap="UniGeneInfoMap">
		select *
		from UniGeneInfo
		where UniProtID = #{uniProtID} 
	</select>
	<select id="selectUniGeneInfoID" parameterType="string" resultMap="UniGeneInfoMap">
		select *
		from UniGeneInfo
		where UniProtID = #{id}
	</select>
	<insert id="insertUniGeneInfo" parameterType="UniGeneInfo">
		insert into UniGeneInfo (UniProtID,Symbol,LocusTag,Synonyms,dbXrefs,chromosome,mapLocation,description,typeOfGene,SymbolFromNomenclature,FullNameFromNomenclature,NomenclatureStatus,OtherDesignations,ModificationDate)
		values (#{uniProtID},#{symbol},#{locusTag},#{synonyms},#{dbXrefs},#{chromosome},#{mapLocation},#{description},#{typeOfGene},#{symNome},#{fullNameNome},#{nomStat},#{otherDesign},#{modDate})
	</insert>
	<update id="updateUniGeneInfo" parameterType="UniGeneInfo">
		update UniGeneInfo
		<set>
			<if test="uniProtID !=null">
				UniProtID = #{uniProtID},
			</if>
			<if test="symbol !=null">
				Symbol = #{symbol},
			</if>
			<if test="locusTag !=null">
				LocusTag = #{locusTag},
			</if>
			<if test="synonyms !=null">
				Synonyms = #{synonyms},
			</if>
			<if test="dbXrefs !=null">
				dbXrefs = #{dbXrefs},
			</if>
			<if test="chromosome !=null">
				chromosome = #{chromosome},
			</if>
			<if test="mapLocation !=null">
				mapLocation = #{mapLocation},
			</if>
			<if test="description !=null">
				description = #{description},
			</if>
			<if test="typeOfGene !=null">
				typeOfGene = #{typeOfGene},
			</if>
			<if test="symNome !=null">
				SymbolFromNomenclature = #{symNome},
			</if>
			<if test="fullNameNome !=null">
				FullNameFromNomenclature = #{fullNameNome},
			</if>
			<if test="nomStat !=null">
				NomenclatureStatus = #{nomStat},
			</if>
			<if test="otherDesign !=null">
				OtherDesignations = #{otherDesign},
			</if>
			<if test="modDate !=null">
				ModificationDate = #{modDate}
			</if>
		</set>
		where UniProtID = #{uniProtID}
	</update>
	
	<resultMap id="UniProtIDMap" type="UniProtID">
		<result property="uniID" column="UniID"/>
		<result property="dbInfo" column="DataBaseInfo"/>
	</resultMap>
	<select id="selectUniProtID" parameterType="UniProtID" resultMap="UniProtIDMap">
		select *
		from UniProtID
		<where>
			<if test="uniID !=null">
				UniID = #{uniID} 
			</if>
			<if test="accessID !=null">
				and accessID = #{accessID} 
			</if>
			<if test="taxID !=null and taxID !=0">
				and TaxID = #{taxID} 
			</if>
			<if test="dbInfo !=null">
				and DataBaseInfo = #{dbInfo}
			</if>
		</where>
	</select>
	<insert id="insertUniProtID" parameterType="UniProtID">
		insert into UniProtID (TaxID,UniID,accessID,DataBaseInfo)
		values (#{taxID},#{uniID},#{accessID},#{dbInfo})
	</insert>
	<update id="updateUniProtID" parameterType="UniProtID">
		update UniProtID 
		<set>
			<if test="taxID !=null and taxID !=0">
				TaxID = #{taxID},
			</if>
			<if test="uniID !=null">
				UniID = #{uniID},
			</if>
			<if test="dbInfo !=null">
				DataBaseInfo = #{dbInfo},
			</if>
			<if test="accessID !=null">
				accessID = #{accessID} 
			</if>
		</set>
		<where>
			<if test="uniID !=null">
				and UniID = #{uniID} 
			</if>
			<if test="accessID !=null">
				and accessID = #{accessID} 
			</if>
		</where>
	</update>	
	
 
	
	
	<resultMap id="Go2TermMap" type="Go2Term">
	</resultMap>
	<select id="selectGo2Term" parameterType="Go2Term" resultMap="Go2TermMap">
		select *
		from Go2Term
		<where>
			<if test="GoIDquery !=null">
				GoIDquery = #{GoIDquery} 
			</if>
			<if test="GoID !=null">
				and GoID = #{GoID} 
			</if>
			<if test="GoFunction !=null">
				and GoFunction = #{GoFunction} 
			</if>
	    </where> 
	</select>
	<insert id="insertGo2Term" parameterType="Go2Term">
		insert into Go2Term (GoIDquery,GoID,GoTerm,GoFunction)
		values (#{GoIDquery},#{GoID},#{GoTerm},#{GoFunction})
	</insert>
	<update id="updateGo2Term" parameterType="Go2Term">
		update Go2Term 
		<set>
			<if test="GoIDquery !=null">
				GoIDquery = #{GoIDquery},
			</if>
			<if test="GoID !=null">
				GoID = #{GoID},
			</if>
			<if test="GoFunction !=null">
				GoFunction = #{GoFunction},
			</if>
			<if test="GoTerm !=null">
				GoTerm = #{GoTerm}
			</if>
		</set>
		<where>
			<if test="GoIDquery !=null">
				GoIDquery = #{GoIDQuery} 
			</if>
			<if test="GoID !=null">
				and GoID = #{GoID} 
			</if>
			<if test="GoFunction !=null">
				and GoFunction = #{GoFunction} 
			</if>
	    </where> 
	</update>	
	
	
	
	<resultMap id="BlastInfoMap" type="BlastInfo">
	</resultMap>
	<select id="selectBlastInfo" parameterType="BlastInfo" resultMap="BlastInfoMap">
		select *
		from BlastInfo
		<where>
			<if test="queryID !=null">
				queryID = #{queryID}
			</if>
			<if test="queryTax !=null and queryTax!=0">
				and queryTax = #{queryTax}
			</if>
			<if test="queryDB !=null">
				and queryDB = #{queryDB}
			</if>
			<if test="subjectID !=null">
				and subjectID = #{subjectID}
			</if>
			<if test="subjectTax !=null and subjectTax!=0">
				and subjectTax = #{subjectTax}
			</if>
			<if test="subjectDB !=null">
				and subjectDB = #{subjectDB}
			</if>
			<if test="subjectTab !=null">
				and subjectTab = #{subjectTab}
			</if>
	    </where> 
	</select>
	<insert id="insertBlastInfo" parameterType="BlastInfo">
		insert into BlastInfo (queryID,queryTax,queryDB,subjectID,subjectTax,subjectDB,identities,evalue,blastDate,subjectTab)
		values (#{queryID},#{queryTax},#{queryDB},#{subjectID},#{subjectTax},#{subjectDB},#{identities},#{evalue},#{blastDate},#{subjectTab})
	</insert>
	<update id="updateBlastInfo" parameterType="BlastInfo">
		update BlastInfo 
		<set>
			<if test="queryTax != null and queryTax != 0">
				queryTax = #{queryTax},
			</if>
			<if test="queryDB != null">
				queryDB = #{queryDB},
			</if>
			<if test="subjectID != null and subjectID != 0 ">
				subjectID = #{subjectID},
			</if>
			<if test="subjectTax != null and subjectTax != 0">
				subjectTax = #{subjectTax},
			</if>
			<if test="subjectDB != null">
				subjectDB = #{subjectDB},
			</if>
			<if test="identities != null">
				identities = #{identities},
			</if>
			<if test="evalue != null">
				evalue = #{evalue},
			</if>
			<if test="blastDate != null">
				blastDate = #{blastDate},
			</if>
			<if test="subjectTab != null">
				subjectTab = #{subjectTab} 
			</if>
		</set>
		<where>
			<if test="queryID != null">
				queryID = #{queryID}
			</if>
			<if test="queryTax != null and queryTax != 0">
				and queryTax = #{queryTax}
			</if>
			<if test="subjectTax != null and subjectTax != 0">
				and subjectTax = #{subjectTax}
			</if>
	    </where> 
	</update>	
	
	
	<resultMap id="TaxInfoMap" type="TaxInfo">
	</resultMap>
	<select id="selectTaxInfo" parameterType="TaxInfo" resultMap="TaxInfoMap">
		select *
		from taxInfo
		<where>
			<if test="taxID !=null and taxID !=0">
				taxID = #{taxID}
			</if>
			<if test="chnName !=null">
				and chnName = #{chnName}
			</if>
			<if test="comName !=null">
				and comName = #{comName}
			</if>
			<if test="latin !=null">
				and latin = #{latin}
			</if>
			<if test="abbr !=null">
				and abbr = #{abbr}
			</if>
	    </where> 
	</select>
	<insert id="insertTaxInfo" parameterType="TaxInfo">
		insert into taxInfo (taxID,chnName,comName,latin,abbr)
		values (#{taxID},#{chnName},#{comName},#{latin},#{abbr})
	</insert>
	<update id="updateTaxInfo" parameterType="TaxInfo">
		update taxInfo
		<set>
			<if test="chnName !=null">
				chnName = #{chnName},
			</if>
			<if test="comName !=null">
				comName = #{comName},
			</if>
			<if test="latin !=null">
				latin = #{latin},
			</if>
			<if test="abbr !=null">
				abbr = #{abbr}
			</if>
		</set>
		where taxID = #{taxID} 
	</update>	
	
</mapper>